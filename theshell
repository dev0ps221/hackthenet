#!/usr/bin/env python3

from core.classes.Shell import Shell,Command
from core.keyboard import *

if __name__ == '__main__':
    shell = Shell()
    shell.addCommand('exit',Command('exit',lambda self : self.shell.stop()))
    shell.addCommand('quit',Command('quit',lambda self : self.shell.stop()))
    shell.addCommand('pymod',Command('pymod',lambda self : print(self.shell)))
    shell.loop()